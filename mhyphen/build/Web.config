<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Booking-Redirect" stopProcessing="true">
                    <match url="booking" />
                    <action type="Rewrite" url="/" />
                </rule>
                <rule name="Home-Redirect" stopProcessing="true">
                    <match url="home" />
                    <action type="Rewrite" url="/" />
                </rule>
                <rule name="Ticket-Redirect" stopProcessing="true">
                    <match url="tickets" />
                    <action type="Rewrite" url="/" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>